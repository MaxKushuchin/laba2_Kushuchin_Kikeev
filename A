#include <iostream>
#include <set> 

int main()
{
    int n, num;
    std::cin >> n;  // Ввод количества чисел

    std::set<int> coll;  // Создание множества для хранения уникальных чисел

    for(int i = 0; i < n; ++i){
        std::cin >> num;  // Ввод числа
        coll.insert(num);  // Добавление числа в множество
    }

    std::cout << coll.size();  // Вывод количества уникальных чисел
    return 0;
}
