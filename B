#include <iostream>
#include <string>
#include <vector>
#include <map>

int main() {
    typedef std::map<int, std::vector<std::string>> IntVectMap;
    IntVectMap coll;

    int num;
    std::string name;

    while (std::cin >> num >> name) {
        coll[num].push_back(name);
    }

    for (int i = 9; i <= 11; ++i) {
        for (const auto& name : coll[i]) {
            std::cout << i << " " << name << std::endl;
        }
    }

    return 0;
}
